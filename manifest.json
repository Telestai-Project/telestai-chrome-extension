{
  "action": {
    "default_popup": "./popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://blockbook.telestai.io/*"],
      "js": ["content_script.js"]
    }
  ],
  "description": "An extension that enables the user to create Digital Signatures using a Telestai address",
  "icons": { "128": "telestai128.png" },
  "manifest_version": 3,
  "name": "Zeroa by Telestai",
  "permissions": ["storage", "activeTab"],
  "version": "1.4.1"
}
